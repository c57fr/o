{
    "name": "grcote7/o",
    "description": "O",
    "homepage": "http://o",
    "keywords": ["o", "october", "cms", "octobercms", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Lionel COTE",
            "email": "GrCOTE7@gmail.com",
            "role": "Founder"
        }
    ],
    "support": {
        "issues": "https://github.com/c57fr/o/issues",
        "forum": "https://github.com/c57fr/o/issues",
        "docs": "https://github.com/c57fr/o/blob/master/README.md",
        "irc": "irc://irc.freenode.net/october",
        "source": "https://github.com/c57fr/o"
    },
    "require": {
        "php": ">=7.0",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "october/rain": "~1.0",
        "october/system": "~1.0",
        "october/backend": "~1.0",
        "october/cms": "~1.0",
        "laravel/framework": "5.5.*",
        "wikimedia/composer-merge-plugin": "dev-master"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.7",
        "phpunit/phpunit": "~6.5",
        "phpunit/phpunit-selenium": "~1.2",
        "meyfa/phpunit-assert-gd": "1.1.0",
        "squizlabs/php_codesniffer": "3.*",
        "jakub-onderka/php-parallel-lint": "^1.0"
    },
    "autoload-dev": {
        "classmap": [
            "modules/tests/TestCase.php",
            "modules/tests/UiTestCase.php",
            "modules/tests/PluginTestCase.php"
        ]
    },
    "scripts": {
        "post-create-project-cmd": [
            "php artisan key:generate",
            "php artisan package:discover"
        ],
        "post-update-cmd": [
            "php artisan october:util set build",
            "php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "platform": {
            "php": "7.0"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "merge-plugin": {
            "include": [
                "plugins/*/*/composer.json"
            ],
            "recurse": true,
            "replace": false,
            "merge-dev": false
        }
    }
}
